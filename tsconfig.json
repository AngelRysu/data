{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./data",
    "rootDir": "./src"
  },
  "include": ["src"],
  "exclude": [
    "node_modules"
  ]
}